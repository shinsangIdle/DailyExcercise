result-view{
	match : RecommendResult(this)
	
	message{
		template ("오늘의 추천 루틴입니다. 원하는 루틴을 선택해주세요.")
	}
	
	
	render{
		
		list-of (this.routine){
			where-each (rou){
				
				layout-macro (RecommendCard){
					param (routine){
						expression (rou)
					}
				}
				
			}
		}
		
	}
	
	
	conversation-drivers{
		conversation-driver{
			template ("데드리프트 운동 카운트 해줘")
		}
		conversation-driver{
			template ("데드리프트 시작")
		}
	}
	
	
}


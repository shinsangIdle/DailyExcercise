result-view {
	match : OriginalGrade(result)
	
	message{
		template ("등급 확인 결과입니다.")
	}
	
	render{
		//	if (result.isValid == "up"){ // Result�� 1개 이상일 경우
		layout{
			section{
			
				content{
					paragraph{
						value("당신의 등급은 #{value(result)} 입니다.")
					}
					image-card{
						title-area{
							slot2{
								paragraph("언제나 열심히")
							}
						}
						aspect-ratio(9:16)
						image-url(images/start1.gif)
					}
					
					if(value(result)=='중급'){
						paragraph{
							value("조금 더 열심히 열심히 해야 하겠네요.")
						}
          	
					}else-if (value(result)=='상급'){
						paragraph{
							value("당신이 최고예요.")
						}
					}else{
						paragraph{
							value("많이 노력 해야 겠네요.")
						}
					}
				}
			}
		}
	}
}
action (FindCount) {
  type (Search)
  description (키값으로 해당되는 오디오 찾기)
  collect {
    input (searchTerm) {
      type (SearchTerm)
      min (Required) max (One)
    }
    
    input(setNum){
      type(SetNum)
      min (Optional) max (One)
    }

    input(power){
      type(Power)
      min(Optional) max (One)
    }
  }
  
  output (CountAudio)
}

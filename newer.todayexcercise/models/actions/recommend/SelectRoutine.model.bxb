action (SelectRoutine) {
  description (__DESCRIPTION__)
  type(Search)
  collect {
    input (routineNum) {
      type (RoutineNum)
      min (Required) max (One)
    }
    input (routine) {
      type (Routine)
      min (Required) max (One)
      default-init {
        intent {
          goal: GetRoutine
          value: $expr(routineNum)
        }
        intent{
          goal: SaveRoutine
          value: $expr(routine)
        }
      }
      
    }
  }
  output (Routine)
}
